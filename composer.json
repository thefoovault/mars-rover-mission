{
  "name": "thefoovault/mars-rover-mission",
  "autoload": {
    "psr-4": {
      "MarsRoverMission\\": "src/MarsRoverMission",
      "Shared\\": "src/Shared",
      "MarsRoverMissionApi\\": "apps/MarsRoverMissionApi/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "extra": {
    "bin-dir": "apps/MarsRoverMissionApi/bin",
    "config-dir": "apps/MarsRoverMissionApi/config",
    "src-dir": "apps/MarsRoverMissionApi/src",
    "var-dir": "apps/MarsRoverMissionApi/var",
    "public-dir": "apps/MarsRoverMissionApi/public"
  },
  "require": {
    "symfony/flex": "^1.13",
    "symfony/framework-bundle": "^5.2",
    "symfony/dotenv": "^5.2",
    "symfony/yaml": "^5.2",
    "symfony/console": "^5.2",
    "symfony/messenger": "^5.2",
    "doctrine/annotations": "^1.0",
    "phpdocumentor/reflection-docblock": "^5.2",
    "symfony/serializer": "^5.2",
    "symfony/property-access": "^5.2",
    "symfony/property-info": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
